{
  "name": "syncot",
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@types/ioredis": "^4.0.15",
    "@types/jest": "^24.0.18",
    "@types/lolex": "^3.1.1",
    "@types/sockjs": "^0.3.31",
    "@types/sockjs-client": "^1.1.1",
    "@types/ws": "^6.0.3",
    "husky": "^3.0.4",
    "ioredis": "^4.14.1",
    "jest": "^24.9.0",
    "jest-extended": "^0.11.2",
    "lerna": "^3.16.4",
    "lolex": "^5.1.0",
    "opentracing": "^0.14.4",
    "prettier": "^1.18.2",
    "readable-stream": "^3.4.0",
    "redis-server": "^1.2.2",
    "rimraf": "^3.0.0",
    "sockjs": "^0.3.19",
    "sockjs-client": "^1.4.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3",
    "ws": "^7.1.2"
  },
  "author": "Greg Kubisa <gkubisa@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SyncOT/SyncOT.git"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "clean": "rimraf packages/*/lib **/*.tsbuildinfo",
    "prettier": "prettier --list-different 'packages/*/src/**/*' 'packages/*/*.json' '*.json' '*.MD' '.prettierrc'",
    "tslint": "tslint --project .",
    "tsc": "tsc -b packages/*",
    "test": "jest",
    "build": "npm run clean && npm run prettier && npm run tslint && npm run tsc && npm test"
  },
  "husky": {
    "hooks": {
      "commit-msg": "[ $(git rev-parse --abbrev-ref HEAD) != 'master' ] || commitlint --edit $HUSKY_GIT_PARAMS",
      "pre-commit": "npm run build"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@syncot/anonymous-auth-service": "file:packages/anonymous-auth-service",
    "@syncot/auth": "file:packages/auth",
    "@syncot/auth-client": "file:packages/auth-client",
    "@syncot/client-storage": "file:packages/client-storage",
    "@syncot/connection": "file:packages/connection",
    "@syncot/event-loop": "file:packages/event-loop",
    "@syncot/ioredis-subscriber": "file:packages/ioredis-subscriber",
    "@syncot/memory-client-storage": "file:packages/memory-client-storage",
    "@syncot/presence": "file:packages/presence",
    "@syncot/presence-client": "file:packages/presence-client",
    "@syncot/redis-presence-service": "file:packages/redis-presence-service",
    "@syncot/tson": "file:packages/tson",
    "@syncot/tson-socket-stream": "file:packages/tson-socket-stream",
    "@syncot/tson-sockjs-stream": "file:packages/tson-sockjs-stream",
    "@syncot/tson-websocket-stream": "file:packages/tson-websocket-stream",
    "@syncot/type": "file:packages/type",
    "@syncot/util": "file:packages/util"
  }
}
