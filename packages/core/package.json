{
  "name": "@syncot/core",
  "version": "0.0.1",
  "description": "The core library of SyncOT.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "rm -rf lib && npm run prettier:write && npm run tslint && npm run tsc",
    "prettier:check": "prettier --list-different 'src/**/*.{ts,tsx}'",
    "prettier:write": "prettier --write 'src/**/*.{ts,tsx}'",
    "tslint": "tslint --project .",
    "tsc": " tsc --project .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Greg Kubisa <gkubisa@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "prettier": "^1.14.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SyncOT/SyncOT.git"
  },
  "dependencies": {
    "tslib": "^1.9.3"
  }
}
